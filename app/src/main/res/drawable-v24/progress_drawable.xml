<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/progress">
        <layer-list>
            <item android:id="@android:id/background">
                <shape>
                    <gradient
                        android:startColor="#ffffff"
                        android:centerColor="#ffffff"
                        android:endColor="#ffffff" />
                    <corners android:radius="10dp" />
                    <stroke
                        android:width="5dp"
                        android:color="#485063" />
                </shape>
            </item>

            <item>
                <clip
                    android:clipOrientation="vertical"
                    android:gravity="bottom">
                    <layer-list>
                        <item
                            android:left="10dp"
                            android:top="2dp"
                            android:right="10dp"
                            android:bottom="10dp">
                            <shape>
                                <gradient
                                    android:startColor="#0859c6"
                                    android:centerColor="#00ffff"
                                    android:endColor="#0859c6" />
                                <corners android:radius="5dp" />
                            </shape>
                        </item>
                        <item
                            android:right="5dp"
                            android:left="5dp"
                            android:top="5dp"
                            android:bottom="5dp">
                            <shape>
                                <gradient
                                    android:startColor="#0859c6"
                                    android:centerColor="#00ffff"
                                    android:endColor="#0859c6" />
                                <corners android:radius="5dp" />
                            </shape>
                        </item>
                    </layer-list>
                </clip>
            </item>
            <item>
                <clip
                    android:clipOrientation="vertical"
                    android:gravity="fill">
                    <layer-list>
                        <item>
                            <shape android:shape="line">
                                <stroke
                                    android:color="#333" />
                            </shape>
                        </item>
                        <item android:top="200dp">
                            <shape android:shape="line">
                                <stroke
                                    android:color="#333" />
                            </shape>
                        </item>
                        <item android:bottom="200dp">
                            <shape android:shape="line">
                                <stroke
                                    android:color="#333" />
                            </shape>
                        </item>
                    </layer-list>
                </clip>
            </item>
        </layer-list>
    </item>
</layer-list>